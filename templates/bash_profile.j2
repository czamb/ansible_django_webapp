# {{ ansible_managed }}
export DJANGO_SECRET_KEY={{ SECRET_KEY }}
export DJANGO_SETTINGS_MODULE={{ django_settings }}
export DJANGO_AWS_ACCESS_KEY_ID="{{ DJANGO_AWS_ACCESS_KEY_ID }}"
